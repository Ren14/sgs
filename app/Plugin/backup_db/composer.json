{
    "name": "mirko-pagliai/cakephp-mysql-backup",
    "description": "Mysql Backup plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/cakephp-mysql-backup",
    "type": "cakephp-plugin",
    "license": "AGPL",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.2.0 <4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "mirko-pagliai/reflection": "^1.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MysqlBackup\\": "src"
        },
        "files": [
            "src/Core/global_functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "MysqlBackup\\Test\\": "tests"
        }
    }
}
